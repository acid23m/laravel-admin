{
    "name": "acid23m/laravel-admin",
    "description": "Admin panel for Laravel application.",
    "type": "laravel-extension",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey P",
            "email": "webmaster.cipa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.4",
        "ext-sqlite3": "*",
        "ext-intl": "*",
        "ext-curl": "*",
        "laravel/framework": "^7.0",
        "dg/adminer-custom": "^1.21",
        "hassankhan/config": "^2.1",
        "kyslik/column-sortable": "^6.3",
        "league/glide-laravel": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-glide": "^3.5",
        "spatie/laravel-html": "^2.25",
        "spatie/laravel-image-optimizer": "^1.6",
        "spatie/laravel-sitemap": "^5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "SP\\Admin\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SP\\Admin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\SP\\Admin\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
