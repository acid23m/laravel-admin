{
  "name": "acid23m/laravel-admin",
  "description": "Admin panel for Laravel application.",
  "type": "laravel-extension",
  "keywords": [
    "laravel",
    "admin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey P",
      "email": "webmaster.cipa@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-sqlite3": "*",
    "dg/adminer-custom": "^v1.0",
    "hassankhan/config": "^2.0",
    "illuminate/auth": "^8.0",
    "illuminate/support": "^8.0",
    "kyslik/column-sortable": "^6.0",
    "laravel/ui": "^v3.0",
    "league/glide-laravel": "^1.0",
    "predis/predis": "^v1.0",
    "spatie/laravel-glide": "^3.0",
    "spatie/laravel-html": "^2.0",
    "spatie/laravel-image-optimizer": "^1.0",
    "spatie/laravel-sitemap": "^5.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SP\\Admin\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SP\\Admin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\SP\\Admin\\ServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
