        "daterangepicker": "^3.0",
        "select2": "^4.0",
        "@ttskch/select2-bootstrap4-theme": "^1.3",
        "html5sortable": "^0.9"
    },
    "devDependencies": {
        "cross-env": "^5.1",
        "laravel-mix": "^4.0",
        "lodash": "^4.17",
